{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "declarationMap": true,
    "paths": {
      "@example/components/*": ["example/components/*"],
      "@example/containers/*": ["example/containers/*"],
      "@example/styles/*": ["example/styles/*"],
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@models/*": ["src/models/*"],
      "@types": ["src/types"],
      "@utils/*": ["src/utils/*"]
    },
    "sourceMap": true
  },
  "include": ["example/**/*.ts", "example/**/*.tsx"]
}
